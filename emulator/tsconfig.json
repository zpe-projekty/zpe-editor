{
    "include": [
        "**/*"
    ],
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "declaration": false,
        "sourceMap": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "outDir": "./build",
        "rootDir": "./src",
    }
}